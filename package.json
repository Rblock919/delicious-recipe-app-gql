{
  "name": "recipe-app-gql",
  "version": "2.0.0",
  "description": "GraphQL Apollo-Server (Express) for Ryan Block's recipe repository application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start.dev": "nodemon",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'",
    "lint.probe": "eslint --fix-dry-run 'src/**/*.ts'",
    "preinstall": "rimraf node_modules",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rblock919/delicious-recipe-app-gql.git"
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "typescript"
  ],
  "author": "Ryan Block",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rblock919/delicious-recipe-app-gql/issues"
  },
  "homepage": "https://github.com/Rblock919/delicious-recipe-app-gql#readme",
  "devDependencies": {
    "@rblock919/eslint-config": "^1.1.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/node-fetch": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.6.9",
    "apollo-server": "^2.9.13",
    "apollo-server-express": "^2.9.14",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "13.1.0",
    "npm": "6.13.2"
  }
}
