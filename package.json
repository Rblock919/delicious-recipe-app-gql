{
  "name": "recipe-app-gql",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server (Express) delicious-recipe-app application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev src/server.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'",
    "lint:probe": "eslint --fix-dry-run 'src/**/*.ts'",
    "preinstall": "rm -rf node_modules",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rblock919/delicious-recipe-app-gql.git"
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "typescript"
  ],
  "author": "Ryan Block",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rblock919/delicious-recipe-app-gql/issues"
  },
  "homepage": "https://github.com/Rblock919/delicious-recipe-app-gql#readme",
  "devDependencies": {
    "@rblock919/eslint-config": "^1.1.10",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.8",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3",
    "wiredep": "^4.0.0"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.6.9",
    "apollo-server": "^2.13.1",
    "apollo-server-express": "^2.13.1",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.15"
  },
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.2"
  }
}
